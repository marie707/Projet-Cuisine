function shorten(text){var ret=text;var screenWidth=$(window).width();var lstChar=/\W$/;if(screenWidth>=320&&screenWidth<=369){maxLength=45;}else if(screenWidth>=370&&screenWidth<=399){maxLength=50;}else if(screenWidth>=400&&screenWidth<=432){maxLength=55;}else if(screenWidth>=480&&screenWidth<=568){maxLength=75;}else if(screenWidth>568){maxLength=ret.length;}
if(ret.length>maxLength){ret=ret.substr(0,maxLength);var ret_LastSpace=ret.lastIndexOf(" ");ret=ret.substr(0,ret_LastSpace);ret=ret.replace(lstChar,'')+'...';}
return ret;}
function resizeLibelle(){$('.resizeLibelle').each(function(index){$(this).html(shorten($(this).attr("data-libelle_cours")));});}
window.addEventListener('resize',resizeLibelle,false);
