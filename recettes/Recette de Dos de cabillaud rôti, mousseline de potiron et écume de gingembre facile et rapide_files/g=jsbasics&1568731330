return returnElements;}}
btn.addEvent(window,'load',function(){btn.init();});;function validateEmail(fld,message)
{var errorVE="";var regExpBeginning=/^\s+/;var regExpEnd=/\s+$/;var tfld=fld.replace(regExpBeginning,"").replace(regExpEnd,"");var emailFilter=/^[^@]+@[^@.]+\.[^@]*\w\w$/;var illegalChars=/[\(\)\<\>\,\;\:\\\"\[\]]/;if(document.getElementById(fld).value=="")
{return false;}
else if(!emailFilter.test(document.getElementById(fld).value))
{return false;}
else if(document.getElementById(fld).value.match(illegalChars))
{return false;}
return true;}
function check_an_email(field_name,message)
{var email=document.getElementById(field_name);if(!validateEmail(field_name,message))
{alert(message);email.focus;return false;}
else
{var filter=/^([\w-]+(?:\.[\w-]+)*)+([\+([\w-]+(?:\.[\w-]+)*]?)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;if(!filter.test(email.value))
{alert(message);email.focus;return false;}}
return true;}
function suppAccents(str){var src=new Array("À","Á","Â","Ã","Ä","Å","à","á","â","ã","ä","å","Ò","Ó","Ô","Õ","Ö","Ø","ò","ó","ô","õ","ö","ø","È","É","Ê","Ë","è","é","ê","ë","Ç","ç","Ì","Í","Î","Ï","ì","í","î","ï","Ù","Ú","Û","Ü","ù","ú","û","ü","ÿ","Ñ","ñ");var rep=new Array("a","a","a","a","a","a","a","a","a","a","a","a","o","o","o","o","o","o","o","o","o","o","o","o","e","e","e","e","e","e","e","e","c","c","i","i","i","i","i","i","i","i","u","u","u","u","u","u","u","u","y","n","n");if(str&&str!=""){for(x=0;x<10;x++)
for(var i=0;i<src.length;i++)
str=str.replace(src[i],rep[i]);}
return str;}
function transf_str_url(str){var rep=new RegExp(" ","g");str=str.replace(/^\s+/g,'').replace(/\s+$/g,'').replace("'","-");url=str.replace(rep,"-");url=url.toLowerCase();return url}
